APP_NAME=Magnifiq
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=magnifiq
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_SSLMODE=prefer

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis

CACHE_STORE=redis
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

OCTANE_SERVER=swoole
OCTANE_HTTPS=false
OCTANE_MAX_REQUESTS=500
OCTANE_WATCH=true
VITE_PORT=5173

MAIL_MAILER=mailgun
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Mailgun Configuration (EU Server)
MAILGUN_DOMAIN=
MAILGUN_SECRET=
MAILGUN_ENDPOINT=https://api.eu.mailgun.net

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# AI Provider Configuration
# Default provider for all AI features. Options: openai, openrouter, replicate
AI_DEFAULT_PROVIDER=openai

# Per-feature provider selection (defaults to AI_DEFAULT_PROVIDER)
AI_CHAT_DRIVER=openai
AI_VISION_DRIVER=openai
AI_IMAGE_GENERATION_DRIVER=replicate

# Per-feature model selection
AI_CHAT_MODEL=openai/gpt-5
AI_VISION_MODEL=openai/gpt-4.1

# Photo Studio default model (optional - defaults to google/gemini-2.5-flash-image)
# PHOTO_STUDIO_DEFAULT_MODEL=google/gemini-2.5-flash-image

# OpenAI Provider Configuration (chat, vision)
OPENAI_API_KEY=
OPENAI_REQUEST_TIMEOUT=120

# OpenRouter Provider Configuration (fallback option)
OPENROUTER_API_KEY=
OPENROUTER_API_ENDPOINT=https://openrouter.ai/api/v1/
OPENROUTER_API_TIMEOUT=120
OPENROUTER_API_TITLE=magnifiq
OPENROUTER_API_REFERER=https://example.com

# Replicate Provider Configuration (image generation)
REPLICATE_API_KEY=
REPLICATE_API_ENDPOINT=https://api.replicate.com/v1/
REPLICATE_API_TIMEOUT=60
REPLICATE_POLLING_TIMEOUT=300
REPLICATE_POLLING_INTERVAL=2.0

# Photo Studio Storage
PHOTO_STUDIO_GENERATION_DISK=s3
